    "web": "http://github.com/garycourt/JSV/issues"
  },
  "repositories": [
    {
      "type": "git",
      "url": "git://github.com/garycourt/JSV.git"
    }
  ],
  "dependencies": [],
  "main": "lib/jsv.js",
  "keywords": [
    "json",
    "schema",
    "validator"
